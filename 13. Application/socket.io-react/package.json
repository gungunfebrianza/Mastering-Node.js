{
  "name": "socketio-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5",
    "socket.io": "^2.5.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
